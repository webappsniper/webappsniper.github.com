{
    "env": {
        "browser": true,
        // "es6": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": 0,
        // "linebreak-style": [
        //     "error",
        //     "unix"
        // ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-undef": 0,
        "no-console": 0,
        "linebreak-style": 0,
        "no-mixed-spaces-and-tabs": 0,
        "no-useless-escape": 0,
    }
}